%div
  .page-title
    .title_left
      %h3 Listing IMAP Settings
    .title_right
      .col-md-12.col-sm-12.col-xs-12.text-right
        = link_to new_imap_setting_path do
          %button.btn.btn-primary New IMAP Settings
.clearfix
.x_panel
  %p Add IMAP settings to read replied emails.
  = render partial: 'shared/stripped_dynamic_table', locals: { collection: @imap_settings, fields: ['address', 'port', 'email'], actions: ['edit', 'destroy'] }
